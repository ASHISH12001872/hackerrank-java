import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
         int a = scan.nextInt();
         int b = scan.nextInt();
        System.out.println(a+b); 
    }
}
