import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
 Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        if (a>0) {
            float b = a;
            System.out.println(b);
        }
        else
        {

            System.out.println("Invalid Input");
        }
           }
}
